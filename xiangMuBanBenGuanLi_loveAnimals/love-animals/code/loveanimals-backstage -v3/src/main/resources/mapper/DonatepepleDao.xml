<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loveanimals.loveanimals.dao.admin.DonatepeopleDao">

    <select id="findDonateByBuserid" resultType="com.loveanimals.loveanimals.po.Donatepeople" parameterType="java.lang.Integer">
        select *
        FROM donatepeople
        WHERE donatepeople.buserid=#{buserid}
    </select>

</mapper>