<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loveanimals.loveanimals.dao.admin.VolunteerDao">

    <select id="findVolunteerByBuserid" resultType="com.loveanimals.loveanimals.po.Volunteer" parameterType="java.lang.Integer">
        select *
        FROM volunteertable
        WHERE volunteertable.buserid=#{buserid}
    </select>

</mapper>